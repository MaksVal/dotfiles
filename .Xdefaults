! URxvt
! URxvt.font:                    xft:Droid Sans Mono:size=11:antialias=true:hinting=false
URxvt.font:                    xft:Hack:size=11:antialias=true:hinting=true
! URxvt.boldFont:                xft:Droid Sans Bold:pixelsize=15:antialias=true:hinting=false
URxvt.boldfont:			xft:terminus:size=11:antialias=true:hinting=true

!:antialias=true
 URxvt.iconFile:	     	/usr/share/icons/utilities-terminal.svg

! URxvt.perl-lib:		    /home/mgordeev/.urxvt/perl/


 URxvt*scrollBar:	    	true
 URxvt.scrollstyle: 		next
 URxvt*scrollTtyOutput:		false
 URxvt*scrollWithBuffer:	true
 URxvt*secondaryWheel:		true

 URxvt*saveLines:	    	13200

 URxvt.perl-ext-common:  	default,tabbed,matcher,url-select,keyboard-select,clipboard,font-size
 URxvt.keysym.M-Escape:		perl:keyboard-select:activate
 URxvt.keysym.M-s: 		    perl:keyboard-select:search
! URxvt.keysym.C-Up:     	perl:font-size:increase
! URxvt.keysym.C-Down:   	perl:font-size:decrease
 URxvt.keysym.C-M-Up:   	perl:font-size:incglobal
 URxvt.keysym.C-M-Down: 	perl:font-size:decglobal
! URxvt.keysym.C-S-c:	 	perl:clipboard:copy
! URxvt.keysym.C-S-v: 		perl:clipboard:paste
 URxvt.keysym.M-c:   		perl:clipboard:copy
 URxvt.keysym.M-v:   		perl:clipboard:paste
! URxvt.keysym.M-C-v: 		perl:clipboard:paste_escaped
 URxvt.keysym.M-u:	      	perl:url-select:select_next


 URxvt.url-select.launcher:	chromium-browser
 URxvt.matcher.button:   	1
 URxvt.url-select.underline:	true
 URxvt.underlineURLs:   	true

! URxvt*urgentOnBell: 		true

 URxvt*tabbed.tabbar-bg:	0
 URxvt*tabbed.tabbar-fg:	2
 URxvt*tabbed.tab-bg:		0
 URxvt*tabbed.tab-fg:		4

 URxvt*transparent: 		false
 URxvt*shading: 		5


